class Solution:
    def canVisitAllRooms(self, rooms: List[List[int]]) -> bool:
        
        visited = [False] * len(rooms)
        stack = rooms[0]
        visited[0] = True
        keys = set()
        res = 1
        while stack:
            room = stack.pop()
            if visited[room]:
                continue
            else:
                visited[room] = True
                res += 1
                for i in rooms[room]:
                    if not visited[i]:
                        if i not in keys:
                            stack.append(i)
                            keys.add(i)
        
        if len(rooms) == res:
            return True
        return False
